JSON = load_json(get_gvar("24bc437f-fd89-415d-88d2-8dc62164f07b")) # Gvar z pogodÄ…

lfm = 1711332000
cyc = 29.53
test = 0*86400 + 0*3600

now = time()
days_passed = int((now + test - lfm) // 86400)
dsf = days_passed % cyc
ttf = int(cyc - dsf)

if ttf < 1:
    msg = 'Mamy peÅ‚niÄ™ ksiÄ™Å¼yca! ðŸŒ•'
elif ttf == 1:
    msg = f'PozostaÅ‚ {ttf} dzieÅ„ do peÅ‚ni ksiÄ™Å¼yca.'
elif ttf in [2, 3, 4]:
    msg = f'PozostaÅ‚y {ttf} dni do peÅ‚ni ksiÄ™Å¼yca.'
else:
    msg = f'PozostaÅ‚o {ttf} dni do peÅ‚ni ksiÄ™Å¼yca.'

J = JSON["weather"]
using(Q="57a16594-b5d7-488c-9e2f-bc4b7c24e8a0")
text=Q.get_next(days_passed, J)
#<t:{int(now + test)}:f> [{days_passed} | {ttf}]
return f'embed {thumb} -title "Dywinacja na dziÅ› brzmi nastÄ™pujÄ…co:" -color {color} -f "---\n{text}\n---\n\n{msg}" {footer}'
